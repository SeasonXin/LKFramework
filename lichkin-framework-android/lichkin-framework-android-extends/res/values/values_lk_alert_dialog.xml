    <color name="lk_alert_dialog_frame_color">#71CEF0</color>
    <color name="lk_alert_dialog_msg_color">#FFFFFF</color>
    <color name="lk_alert_dialog_btn_color">#01A6E6</color>
    <color name="lk_alert_dialog_btn_color_pressed">#48C6EF</color>
    <color name="lk_alert_dialog_btn_textColor">#FFFFFF</color>
    <color name="lk_loading_dialog_background_color">#6648C6EF</color>
    <!-- dimens -->
    <dimen name="lk_alert_dialog_frame_radius">6dp</dimen>
    <dimen name="lk_loading_dialog_msg_textSize">12sp</dimen>
    <dimen name="lk_alert_dialog_msg_textSize">16sp</dimen>
    <dimen name="lk_alert_dialog_btn_textSize">16sp</dimen>
    <!-- styles -->
    <style name="lk_alert_dialog_style" parent="android:style/Theme.Dialog">
        <item name="android:background">@android:color/transparent</item>
