<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- colors -->
    <color name="lk_btn_list_in_color">#EEEEEE</color>
    <color name="lk_btn_list_btn_itle_color">#FFFFFF</color>
    <!-- styles -->
    <style name="lk_btn_list_out">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="lk_btn_list_in">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/lk_btn_list_in_color</item>
    </style>
    <style name="lk_btn_list_btn_img">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="lk_btn_list_btn_itle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">@color/lk_textColor_dark</item>
        <item name="android:background">@color/lk_btn_list_btn_itle_color</item>
    </style>
</resources>