<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="lk_title">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/lk_foregroundColor</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:textColor">@color/lk_title_textColor</item>
        <item name="android:textSize">@dimen/lk_title_textSize</item>
    </style>
    <style name="lk_alert_dialog_style" parent="android:style/Theme.Dialog">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="lk_btn_list_out">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="lk_btn_list_in">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/lk_btn_list_in_color</item>
    </style>
    <style name="lk_btn_list_btn_img">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="lk_btn_list_btn_itle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">@color/lk_textColor_dark</item>
        <item name="android:background">@color/lk_btn_list_btn_itle_color</item>
    </style>
    <style name="lk_loading_dialog_progressBar" parent="android:style/Widget.ProgressBar">
        <item name="android:indeterminateDrawable">@drawable/layerlist_lk_loading_dialog_progress</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:maxWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:maxHeight">48dp</item>
    </style>
</resources>